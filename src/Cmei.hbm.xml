<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.Cmei" table="cmei">
    <!-- Identificador da classe -->
    <id column="codigocmei" name="codigoCmei">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="nomeentidade" name="nomeEntidade"/>
    <property column="endereco" name="endereco"/>
    <property column="numeroendereco" name="numeroEndereco"/>
    <property column="municipio" name="municipio"/>
    <property column="bairro" name="bairro"/>
    <property column="telefone" name="telefone"/>
    <property column="coordenadoratual" name="coordenadorAtual"/>
    <property column="capacidadealunos" name="capacidadeAlunos"/>
    <property column="vagasdisponiveis" name="vagasDisponiveis"/>
    <property column="numerolotacoes" name="numeroLotacoes"/>
    <property column="servidoresconcursados" name="servidoresConcursados"/>
    <property column="numeroestagiarios" name="numeroEstagiarios"/>
    <property column="numerosalasbercarios" name="numeroSalasBercarios"/>
    <property column="numerosalasmaternal" name="numeroSalasMaternal"/>
    <property column="numerosalaspre" name="numeroSalasPre"/>
  </class>
</hibernate-mapping>
