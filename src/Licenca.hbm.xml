<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.Licenca" table="licenca">
    <!-- Identificador da classe -->
    <id column="codigolicenca" name="codigoLicenca">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="descricaolicenca" name="descricaoLicenca"/>
    <property column="tipolicenca" name="tipoLicenca"/>
  </class>
</hibernate-mapping>
