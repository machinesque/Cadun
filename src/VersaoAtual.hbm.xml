<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.VersaoAtual" table="versaoatual">
    <!-- Identificador da classe -->
    <id column="codigoversaoatual" name="codigoVersaoAtual">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="numeroversao" name="numeroVersao"/>
    <property column="dataversao" name="dataVersao"/>
  </class>
</hibernate-mapping>
