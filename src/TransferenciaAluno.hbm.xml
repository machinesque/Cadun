<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
To change this template, choose Tools | Templates
and open the template in the editor.
-->
<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.TransferenciaAluno" table="transferenciaaluno">
    <!-- Identificador da classe -->
    <id column="codigotransferenciaaluno" name="codigoTransferenciaAluno">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="codigoaluno" name="codigoAluno"/>
    <property column="cpfmae" name="cpfMae"/>
    <property column="cpfpai" name="cpfPai"/>
    <property column="datatransferencia" name="dataTransferencia"/>
    <property column="filiacaomae" name="filiacaoMae"/>
    <property column="filiacaopai" name="filiacaoPai"/>
    <property column="nomealuno" name="nomeAluno"/>
    <property column="matriculaaluno" name="matriculaAluno"/>
    <property column="nomeentidadeanterior" name="nomeEntidadeAnterior"/>
    <property column="nomeentidadeatual" name="nomeEntidadeAtual"/>
    <property column="motivotransferencia" name="motivoTransferencia"/>
  </class>
</hibernate-mapping>