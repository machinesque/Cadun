<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.Contrasenha" table="contrasenha">
    <!-- Identificador da classe -->
    <id column="codigocontrasenha" name="codigoContraSenha">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="contrasenha" name="contraSenha"/>
    <property column="tipocontrasenha" name="tipoContraSenha"/>
  </class>
</hibernate-mapping>
