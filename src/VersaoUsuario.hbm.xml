<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.VersaoUsuario" table="versaousuario">
    <!-- Identificador da classe -->
    <id column="codigoversaousuario" name="codigoVersaoUsuario">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="codigousuario_usuario" name="codigoUsuario_Usuario"/>
    <property column="dataatualizacao" name="dataAtualizacao"/>
    <property column="versaoatual" name="versaoAtual"/>
  </class>
</hibernate-mapping>
