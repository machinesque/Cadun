<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.FuncaoCargo" table="funcaocargo">
    <!-- Identificador da classe -->
    <id column="codigofuncaocargo" name="codigoFuncaoCargo">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="nomefuncaocargo" name="nomeFuncaoCargo"/>
  </class>
</hibernate-mapping>
