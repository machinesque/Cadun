<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.Usuario" table="usuario">
    <!-- Identificador da classe -->
    <id column="codigousuario" name="codigoUsuario">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="nomeusuario" name="nomeUsuario"/>
    <property column="senha" name="senha"/>
    <property column="contrasenha" name="contraSenha"/>
    <property column="temaescolhido" name="temaEscolhido"/>
    <property column="tipocadastro" name="tipoCadastro"/>
    <property column="matricula" name="matricula"/>
  </class>
</hibernate-mapping>
