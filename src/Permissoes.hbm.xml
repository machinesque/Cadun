<hibernate-mapping>
  <class name="modelo.Permissoes" table="permissoes">
    <!-- Identificador da classe -->
    <id column="codigopermissoes" name="codigoPermissoes">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="nomeusuario" name="nomeUsuario"/>
    <property column="cadastraraluno" name="cadastrarAluno"/>
    <property column="cadastrarcmei" name="cadastrarCmei"/>
    <property column="cadastrarfuncaocargo" name="cadastrarFuncaoCargo"/>
    <property column="cadastrarlicenca" name="cadastrarLicenca"/>
    <property column="cadastrarrestricao" name="cadastrarRestricao"/>
    <property column="cadastrarservidor" name="cadastrarServidor"/>
    <property column="cadastrarusuario" name="cadastrarUsuario"/>
    <property column="imprimircmei" name="imprimirCmei"/>
    <property column="imprimiraluno" name="imprimirAluno"/>
  </class>
</hibernate-mapping>
