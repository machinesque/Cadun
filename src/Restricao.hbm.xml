<!-- Banco PostgreSql`-->
<hibernate-mapping>
  <class name="modelo.Restricao" table="restricao">
    <!-- Identificador da classe -->
    <id column="codigorestricao" name="codigoRestricao">
      <generator class="increment"/>
    </id>
    <!-- Propriedades da classe -->
    <property column="descricaorestricao" name="descricaoRestricao"/>
    <property column="tiporestricao" name="tipoRestricao"/>
  </class>
</hibernate-mapping>
